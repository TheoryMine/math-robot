
## targets

default: T_1
images: 
test: T_1

all: images test


## global settings

## ISABELLE_TOOL = /Applications/Isabelle2009-2.app/Isabelle/bin/isabelle

HEAPNAME = tm_initial_$(SHORT_HOSTNAME)
SRC = $(ISABELLE_HOME)/src
OUT = $(ISABELLE_OUTPUT)
LOG = $(OUT)/log

USEDIR = $(ISABELLE_TOOL) usedir -v true -i false -g false -p 1


T_1: $(LOG)/HOL-T_1.gz

$(LOG)/HOL-T_1.gz: ROOT.ML *.thy *.ML
	@$(USEDIR) $(HEAPNAME) .


## clean

clean:
	@rm -f $(LOG)/HOL-T_1.gz
